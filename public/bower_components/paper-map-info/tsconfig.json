{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "commonjs",
        "preserveConstEnums": true,
        "sourceMap": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "emitDecoratorMetadata": false,
        "removeComments": false
    },
    "filesGlob": [
        "*.ts",
        "typings/**/*.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    },
    "files": [
        "paper-map-info.ts",
        "typings/googlemaps.d.ts",
        "typings/underscore.d.ts"
    ]
}
